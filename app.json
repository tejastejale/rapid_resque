{
  "expo": {
    "name": "Rapid Resque",
    "slug": "argon-free-react-native",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tejas1324.argonfreereactnative",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "description": "Argon React Native, based on Argon Design System. Coded by Creative Tim",
    "extra": {
      "mapboxAccessToken": "pk.eyJ1IjoidGVqYXNjb2RlNDciLCJhIjoiY200d3pqMGh2MGtldzJwczgwMTZnbHc0dCJ9.KyxtwzKWPT9n1yDElo8HEQ",
      "eas": {
        "projectId": "6c3880db-d3b6-4e35-9263-775d935bafc4"
      }
    },
    "android": {
      "package": "com.tejas1324.argonfreereactnative",
      "useNextNotificationsApi": true,
      "googleServicesFile": "./android/app/google-services.json"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "defaultChannel": "default",
          "sounds": ["./assets/alarm.mp3", "./assets/alarm.mp3"],
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ]
  }
}
